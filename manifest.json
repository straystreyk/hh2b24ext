{
  "manifest_version": 3,
  "name": "HH2B24 Провайдер",
  "short_name": "HH2B24",
  "version": "1.0.0",
  "key": "",
  "background": {
    "service_worker": "background/background.ts",
    "type": "module"
  },
  "action": {
    "default_title": "H2B24 Провайдер",
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "src/assets/favicon/image16.png",
      "48": "src/assets/favicon/image48.png",
      "128": "src/assets/favicon/image128.png"
    }
  },
  "icons": {
    "16": "src/assets/favicon/image16.png",
    "48": "src/assets/favicon/image48.png",
    "128": "src/assets/favicon/image128.png"
  },
  "content_scripts": [
    {
      "js": [
        "src/contentScripts/SendBtn/main.tsx"
      ],
      "matches": [
        "https://hh.ru/resume*",
        "https://*.hh.ru/resume*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "identity",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
